<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane stylesheets="@application.css" prefHeight="49.0" prefWidth="83.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SignUpUI">
	<children>
		<Pane id="homePane" prefHeight="600.0" prefWidth="800.0">
			<children>
				<Label layoutX="337.0" layoutY="108.0" text="Sign Up">
					<font>
						<Font name="Segoe UI" size="36.0" />
					</font>
				</Label>
				<TextField layoutX="100.0" layoutY="182.0" prefHeight="40.0" prefWidth="600.0" promptText="Name" />
				<TextField layoutX="100.0" layoutY="246.0" prefHeight="40.0" prefWidth="600.0" promptText="Email ID" />
				<TextField layoutX="99.0" layoutY="308.0" prefHeight="40.0" prefWidth="600.0" promptText="Password" />
				<TextField layoutX="100.0" layoutY="372.0" prefHeight="40.0" prefWidth="600.0" promptText="Confirm Password" />
				<MenuButton layoutX="100.0" layoutY="436.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="600.0" text="Type of User">
					<items>
						<MenuItem mnemonicParsing="false" text="Admin" />
						<MenuItem mnemonicParsing="false" text="Faculty" />
						<MenuItem mnemonicParsing="false" text="Student" />
					</items>
				</MenuButton>
				<Button id="button" layoutX="300.0" layoutY="516.0" mnemonicParsing="false" onAction="#openLogin" prefHeight="50.0" prefWidth="200.0" text="REGISTER">
					<font>
						<Font name="Segoe UI" size="16.0" />
					</font>
				</Button>
			</children>
		</Pane>
	</children>
</AnchorPane>
